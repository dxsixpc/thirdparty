{
  "name": "@dxsixpc/thirdparty",
  "version": "0.1.0",
  "description": "封装的第三方工具库",
  "homepage": "https://dxsixpc.github.io/thirdparty",
  "repository": "https://github.com/dxsixpc/thirdparty",
  "license": "MIT",
  "author": {
    "name": "dxsix",
    "email": "dxsixpc@gmail.com"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "commit": "git add . && git-cz",
    "deploy": "pnpm run docs:build && pnpm run docs:deploy",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "doctor": "father doctor",
    "lint": "pnpm run lint:es && pnpm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install && dumi setup",
    "prepublishOnly": "father doctor && pnpm run test && pnpm run build",
    "start": "pnpm run dev"
  },
  "dependencies": {
    "@dxsixpc/utils": "^1.2.0",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/preset-env": "^7.22.7",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/runtime": "^7.22.6",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@dxsixpc/configs": "^1.2.4",
    "@types/node": "^20.4.1",
    "clipboard": "^2.0.11",
    "commitizen": "^4.0.3",
    "dumi": "^2.2.1",
    "eslint": "^8.44.0",
    "father": "^4.2.3",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "inquirer": "^8.2.5",
    "lint-staged": "^13.2.3",
    "path": "^0.12.7",
    "postcss": "^8.4.25",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.0.3",
    "stylelint": "^14.16.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack": "^5.88.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
