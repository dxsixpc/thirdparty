    "@babel/core": "7.24.9",
    "@babel/preset-env": "7.24.8",
    "@babel/preset-typescript": "7.24.7",
    "@babel/runtime": "7.24.8",
    "@commitlint/cz-commitlint": "19.2.0",
    "@types/crypto-js": "4.2.2",
    "@types/node": "20.14.12",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
